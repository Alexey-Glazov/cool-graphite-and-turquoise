{
  "name": "dark cg turquoise theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#4be1c3"
    },
    "variable.constant": {
      "foreground": "#d17a66bd"
    },
    "variable.defaultLibrary": {
      "foreground": "#e5c07b"
    }
